{
  "name": "ai-knowledge-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "markdownlint src/content/**/*.md",
    "lint:fix": "markdownlint --fix src/content/**/*.md",
    "type-check": "astro check",
    "test:unit": "vitest run --reporter=verbose",
    "test:unit:watch": "vitest --reporter=verbose",
    "test:unit:coverage": "vitest run --coverage --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run test:unit:coverage && npm run test:e2e"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/sitemap": "^3.2.1",
    "astro": "^4.16.11",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "jsdom": "^23.0.1",
    "markdownlint": "^0.35.0",
    "markdownlint-cli": "^0.42.0",
    "playwright": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.6.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}