# PRODUCTION SECRETS FOR REAL ACT TESTING
# ==========================================
# Copy this to .secrets and fill in REAL values for production-like local testing
# 
# WARNING: Never commit this file with real secrets!
# Add .secrets to your .gitignore if not already present

# =============================================================================
# DATABASE CONNECTIONS (Real Database Services)
# =============================================================================

# Option 1: Supabase (Recommended for real testing)
DATABASE_URL=postgresql://postgres:[YOUR_SUPABASE_PASSWORD]@[YOUR_SUPABASE_HOST]:5432/postgres
SUPABASE_URL=https://[YOUR_PROJECT_ID].supabase.co
SUPABASE_ANON_KEY=eyJ[YOUR_SUPABASE_ANON_KEY]
SUPABASE_SERVICE_ROLE_KEY=eyJ[YOUR_SUPABASE_SERVICE_ROLE_KEY]

# Option 2: Local PostgreSQL (for development)
# DATABASE_URL=postgresql://postgres:yourpassword@localhost:5432/ai_knowledge_test

# Option 3: Cloud PostgreSQL (AWS RDS, Google Cloud SQL, etc.)
# DATABASE_URL=postgresql://username:password@your-db-host:5432/database_name

# =============================================================================
# AI API KEYS (Real API Keys - NOT TEST PLACEHOLDERS)
# =============================================================================

# OpenAI API (for content processing, embeddings, etc.)
OPENAI_API_KEY=sk-[YOUR_REAL_OPENAI_API_KEY]
OPENAI_ORG_ID=[YOUR_OPENAI_ORG_ID]

# Anthropic API (for Claude-based operations)
ANTHROPIC_API_KEY=sk-ant-[YOUR_REAL_ANTHROPIC_API_KEY]

# Additional AI Services (if used)
HUGGINGFACE_API_KEY=[YOUR_HUGGINGFACE_TOKEN]
COHERE_API_KEY=[YOUR_COHERE_API_KEY]

# =============================================================================
# EXTERNAL SERVICE INTEGRATIONS
# =============================================================================

# GitHub Integration
GITHUB_TOKEN=ghp_[YOUR_GITHUB_PERSONAL_ACCESS_TOKEN]
GITHUB_APP_ID=[YOUR_GITHUB_APP_ID]
GITHUB_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\n[YOUR_GITHUB_APP_PRIVATE_KEY]\n-----END RSA PRIVATE KEY-----"

# Codecov (for coverage reporting)
CODECOV_TOKEN=[YOUR_CODECOV_TOKEN]

# Security Services
SNYK_TOKEN=[YOUR_SNYK_TOKEN]
SECURITY_SCORECARD_TOKEN=[YOUR_SECURITY_SCORECARD_TOKEN]

# =============================================================================
# MONITORING AND OBSERVABILITY
# =============================================================================

# Application Performance Monitoring
DATADOG_API_KEY=[YOUR_DATADOG_API_KEY]
DATADOG_APP_KEY=[YOUR_DATADOG_APP_KEY]

# Error Tracking
SENTRY_DSN=https://[YOUR_SENTRY_DSN]@[YOUR_SENTRY_ORG].ingest.sentry.io/[PROJECT_ID]
SENTRY_AUTH_TOKEN=[YOUR_SENTRY_AUTH_TOKEN]

# Log Management
LOGDNA_INGESTION_KEY=[YOUR_LOGDNA_KEY]

# =============================================================================
# CLOUD SERVICES AND CDN
# =============================================================================

# AWS (if using S3, CloudFront, etc.)
AWS_ACCESS_KEY_ID=[YOUR_AWS_ACCESS_KEY]
AWS_SECRET_ACCESS_KEY=[YOUR_AWS_SECRET_KEY]
AWS_REGION=us-east-1
AWS_S3_BUCKET=[YOUR_S3_BUCKET_NAME]

# Cloudflare (if using CDN, Workers, etc.)
CLOUDFLARE_API_TOKEN=[YOUR_CLOUDFLARE_TOKEN]
CLOUDFLARE_ZONE_ID=[YOUR_ZONE_ID]

# Vercel (if deploying there)
VERCEL_TOKEN=[YOUR_VERCEL_TOKEN]
VERCEL_ORG_ID=[YOUR_VERCEL_ORG_ID]
VERCEL_PROJECT_ID=[YOUR_VERCEL_PROJECT_ID]

# =============================================================================
# SECURITY AND COMPLIANCE
# =============================================================================

# Security Webhooks (for alerts)
SECURITY_WEBHOOK_URL=https://hooks.slack.com/services/[YOUR_SLACK_WEBHOOK]
SECURITY_ALERT_EMAIL=security-alerts@yourcompany.com

# Vulnerability Database Keys
NVD_API_KEY=[YOUR_NVD_API_KEY]
CVE_API_KEY=[YOUR_CVE_API_KEY]

# =============================================================================
# TESTING AND DEVELOPMENT
# =============================================================================

# Test Environment Configuration
TEST_ENVIRONMENT=act_production_simulation
TEST_USE_REAL_SERVICES=true
TEST_MOCK_AI_APIS=false
TEST_COVERAGE_THRESHOLD=95

# Browser Testing (Playwright, Selenium)
BROWSERSTACK_USERNAME=[YOUR_BROWSERSTACK_USERNAME]
BROWSERSTACK_ACCESS_KEY=[YOUR_BROWSERSTACK_ACCESS_KEY]

# Performance Testing
LIGHTHOUSE_CI_TOKEN=[YOUR_LIGHTHOUSE_CI_TOKEN]

# =============================================================================
# SMTP AND EMAIL (for notifications)
# =============================================================================

# Email Service (SendGrid, Mailgun, etc.)
SENDGRID_API_KEY=SG.[YOUR_SENDGRID_API_KEY]
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASS=[YOUR_SENDGRID_API_KEY]

# =============================================================================
# ADDITIONAL INTEGRATIONS
# =============================================================================

# Search Services (if using Elasticsearch, Algolia, etc.)
ELASTICSEARCH_URL=https://[YOUR_ELASTICSEARCH_CLUSTER]
ELASTICSEARCH_USERNAME=[USERNAME]
ELASTICSEARCH_PASSWORD=[PASSWORD]
ALGOLIA_APP_ID=[YOUR_ALGOLIA_APP_ID]
ALGOLIA_API_KEY=[YOUR_ALGOLIA_API_KEY]

# Analytics
GOOGLE_ANALYTICS_ID=G-[YOUR_GA_ID]
MIXPANEL_TOKEN=[YOUR_MIXPANEL_TOKEN]

# Social Media APIs (if scraping or integrating)
TWITTER_API_KEY=[YOUR_TWITTER_API_KEY]
TWITTER_API_SECRET=[YOUR_TWITTER_API_SECRET]
REDDIT_CLIENT_ID=[YOUR_REDDIT_CLIENT_ID]
REDDIT_CLIENT_SECRET=[YOUR_REDDIT_CLIENT_SECRET]

# =============================================================================
# DEPLOYMENT KEYS
# =============================================================================

# SSH Keys for deployment
DEPLOY_SSH_PRIVATE_KEY="-----BEGIN OPENSSH PRIVATE KEY-----\n[YOUR_DEPLOYMENT_SSH_KEY]\n-----END OPENSSH PRIVATE KEY-----"
DEPLOY_HOST=[YOUR_DEPLOYMENT_HOST]
DEPLOY_USER=[YOUR_DEPLOYMENT_USER]

# Container Registry
DOCKER_USERNAME=[YOUR_DOCKER_USERNAME]
DOCKER_PASSWORD=[YOUR_DOCKER_PASSWORD]
GHCR_TOKEN=[YOUR_GITHUB_CONTAINER_REGISTRY_TOKEN]

# =============================================================================
# ENVIRONMENT SPECIFIC SETTINGS
# =============================================================================

# Local Development Overrides
NODE_ENV=test
PYTHON_ENV=test
DEBUG_MODE=true
VERBOSE_LOGGING=true

# Security Settings
ENABLE_SECURITY_HEADERS=true
ENABLE_CSP=true
ENABLE_HSTS=true

# Performance Settings
ENABLE_CACHING=true
CACHE_TTL=3600
ENABLE_COMPRESSION=true

# Feature Flags
FEATURE_AI_CONTENT_GENERATION=true
FEATURE_REAL_TIME_UPDATES=true
FEATURE_ADVANCED_SEARCH=true